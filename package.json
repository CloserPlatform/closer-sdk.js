{
  "name": "ratel-sdk-js",
  "version": "0.3.8",
  "description": "Ratel JavaScript SDK",
  "main": "dist/ratel-sdk.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "npm run commit-hash && cross-env NODE_ENV=production webpack",
    "build:ci": "npm run commit-hash && cross-env NODE_ENV=development webpack",
    "clean": "rimraf dist node_modules coverage",
    "commit-hash": "git rev-parse HEAD > im/version.txt",
    "docs": "typedoc --out dist/docs --exclude '**/*.spec.ts' ./src",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prebuild": "npm run test",
    "prebuild:ci": "npm run test:ci",
    "pretest": "npm run lint",
    "pretest:ci": "npm run lint",
    "posttest": "istanbul check-coverage --statement 75 --branch 57 --function 59",
    "posttest:ci": "istanbul check-coverage --statement 66 --branch 45 --function 47",
    "start": "python3 -m http.server",
    "test": "karma start --singleRun true",
    "test:ci": "karma start --singleRun true --browsers PhantomJS",
    "test:firefox": "karma start --singleRun true --browsers Firefox",
    "test:chrome": "karma start --singleRun true --browsers ChromeWithFakeUserMedia",
    "test:watch": "karma start --singleRun false",
    "test:watch:firefox": "karma start --singleRun false --browsers Firefox",
    "test:watch:chrome": "karma start --singleRun false --browsers ChromeWithFakeUserMedia",
    "test:grep": "karma run -- --grep"
  },
  "author": "Kajetan Rzepecki <kajtek@idorobots.org>",
  "license": "MIT",
  "dependencies": {
    "webrtc-adapter": "^3.3.4"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.43",
    "cross-env": "^3.1.4",
    "es6-promise": "^3.3.1",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.1",
    "object-assign": "^4.1.1",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.11",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.2.1",
    "watchify": "^3.9.0",
    "webpack": "^1.14.0"
  },
  "publishConfig": {
    "registry": "https://nexus.contactis.pl/repository/npm-internal"
  }
}
