{
  "name": "ratel-sdk-js",
  "version": "0.8.12",
  "repository": "https://git.contactis.pl/ratel/ratel-sdk-js",
  "description": "Ratel JavaScript SDK",
  "main": "dist/ratel-sdk.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "prebuild": "npm run lint",
    "prebuild:ci": "npm run lint",
    "build": "cross-env NODE_ENV=production webpack",
    "build:ci": "cross-env NODE_ENV=development webpack",
    "build-demo-app": "cross-env NODE_ENV=production webpack --config webpack.demo-app.config.js && npm run commit-hash",
    "build-demo-app:ci": "cross-env NODE_ENV=development webpack --config webpack.demo-app.config.js && npm run commit-hash",
    "clean": "rimraf dist node_modules coverage dist-demo-app",
    "commit-hash": "git rev-parse HEAD > dist-demo-app/version.txt",
    "docs": "typedoc --out dist/docs --exclude '**/*.spec.ts' ./src",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "pretest": "npm run lint",
    "pretest:ci": "npm run lint",
    "posttest": "istanbul check-coverage --statement 70 --branch 48 --function 55 --line 70",
    "posttest:ci": "istanbul check-coverage --statement 70 --branch 48 --function 55 --line 70",
    "start": "node node_modules/http-server/bin/http-server ./dist-demo-app -p 8080",
    "test": "karma start --singleRun true",
    "test:ci": "karma start --singleRun true --browsers HeadlessChrome #,HeadlessFirefox",
    "test:firefox": "karma start --singleRun true --browsers Firefox",
    "test:chrome": "karma start --singleRun true --browsers ChromeWithFakeUserMedia",
    "test:watch": "karma start --singleRun false",
    "test:watch:firefox": "karma start --singleRun false --browsers Firefox",
    "test:watch:chrome": "karma start --singleRun false --browsers ChromeWithFakeUserMedia",
    "test:grep": "karma run -- --grep"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "webrtc-adapter": "^6.3.0"
  },
  "peerDependencies": {
    "rxjs": "^6.2.1",
    "@types/webrtc": "0.0.23"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/jquery": "^3.3.2",
    "@types/jqueryui": "^1.12.2",
    "@types/webrtc": "0.0.23",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.4",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "object-assign": "^4.1.1",
    "rimraf": "^2.6.1",
    "rxjs": "^6.2.1",
    "source-map-support": "^0.4.15",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^3.0.5",
    "tslint": "^5.10.0",
    "tslint-immutable": "^4.5.4",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.9.2",
    "watchify": "^3.9.0",
    "webpack": "^3.8.1"
  },
  "publishConfig": {
    "registry": "https://nexus.contactis.pl/repository/npm-internal"
  }
}
