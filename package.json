{
  "name": "ratel-sdk-js",
  "version": "0.1.1",
  "description": "Ratel JavaScript SDK",
  "main": "dist/ratel-sdk.js",
  "scripts": {
    "deps": "typings install",
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "test": "karma start",
    "test-dev": "karma start --no-single-run --browsers 'ChromeWithFakeUserMedia,Firefox,PhantomJS'",
    "build": "npm run commit-hash && webpack && npm run browserify",
    "start": "python3 -m http.server",
    "prepublish": "npm run deps && npm run build",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "commit-hash": "git rev-parse HEAD > im/version.txt",
    "browserify": "browserify im/index.js -o im/bundle.js"
  },
  "author": "Kajetan Rzepecki <kajtek@idorobots.org>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^13.1.0",
    "crypto-js": "^3.1.6",
    "es6-promise": "^3.2.1",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript-preprocessor": "^0.2.1",
    "karma-webpack": "^1.8.0",
    "ts-loader": "^0.8.2",
    "tsify": "^1.0.3",
    "tslint": "^3.15.1",
    "typedoc": "^0.4.5",
    "typedoc-webpack-plugin": "^1.0.9",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "watchify": "^3.7.0",
    "webpack": "^1.13.1"
  },
  "publishConfig": {
    "registry": "https://nexus.contactis.pl/content/repositories/npm-internal"
  }
}
