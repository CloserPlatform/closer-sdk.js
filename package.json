{
  "name": "closer-sdk-js",
  "version": "1.0.7",
  "repository": "https://git.contactis.pl/ratel/ratel-sdk-js",
  "description": "Closer SDK JS",
  "main": "dist/closer-sdk.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish:lib": "npm run build",
    "publish:lib": "npm publish",
    "prebuild": "npm run lint",
    "prebuild:ci": "npm run lint",
    "profile": "webpack --profile --json > profile.json",
    "analyze": "webpack-bundle-analyzer profile.json",
    "build": "webpack --mode=production",
    "build:ci": "webpack --mode=development",
    "build-demo-app": "webpack --config webpack.demo-app.config.js --mode=production && npm run commit-hash",
    "build-demo-app:ci": "webpack --config webpack.demo-app.config.js --mode=development && npm run commit-hash",
    "commit-hash": "node -p \"require('./package.json').version\" > dist-demo-app/version.txt",
    "docs": "typedoc --out dist/docs --exclude '**/*.spec.ts' ./src",
    "demo-app": "npm run build-demo-app:ci && node node_modules/http-server/bin/http-server ./dist-demo-app -p 8080",
    "lint": "tslint --project tsconfig.sdk.json",
    "pretest": "npm run lint",
    "pretest:ci": "npm run lint",
    "coverage": "cat ./coverage/summary.txt",
    "posttest": "npm run coverage",
    "posttest:ci": "npm run coverage",
    "test": "karma start --singleRun true",
    "test:ci": "karma start --singleRun true --browsers HeadlessChrome #,HeadlessFirefox",
    "test:firefox": "karma start --singleRun true --browsers Firefox",
    "test:chrome": "karma start --singleRun true --browsers ChromeWithFakeUserMedia",
    "test:watch": "karma start --singleRun false",
    "test:watch:firefox": "karma start --singleRun false --browsers Firefox",
    "test:watch:chrome": "karma start --singleRun false --browsers ChromeWithFakeUserMedia",
    "test:grep": "karma run -- --grep",
    "ci-deploy": "npm run build:ci && npm run build-demo-app:ci",
    "ci-test": "npm run test"
  },
  "dependencies": {
    "@swagger/spinner": "^0.1.483",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "webrtc-adapter": "^7.7.0"
  },
  "peerDependencies": {
    "rxjs": "^6.6.2",
    "@types/webrtc": "^0.0.26"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.12",
    "@types/jquery": "^3.5.1",
    "@types/jqueryui": "^1.12.13",
    "@types/webrtc": "^0.0.26",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^3.6.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.1",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "rxjs": "^6.6.2",
    "source-map-support": "^0.4.15",
    "terser-webpack-plugin": "^4.1.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
    "tslint-immutable": "^6.0.1",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-karma-die-hard": "^1.0.4"
  },
  "publishConfig": {
    "registry": "https://nexus.ratel.io/repository/npm-internal/"
  }
}
